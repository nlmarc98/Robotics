package nl.ru.ai.MMJV;

import lejos.robotics.subsumption.Arbitrator;
import lejos.robotics.subsumption.Behavior;

public class ProjectAlpaK {	
	
	public static void main(String[] args) {
		SampleRetrieval sr = new SampleRetrieval();
		Filter sample = new Filter(sr);
		Behavior b1 = new FollowPathCounterClockwise(sample);
		Behavior b2 = new CrossLine(sample);
		Behavior b3 = new FollowPathClockwise(sample);
		Behavior b4 = new DriveForward(sample);
		Behavior [] bArray = {b1, b2, b3, b4};
	    Arbitrator arby = new Arbitrator(bArray);
	    arby.start();
	}
}
